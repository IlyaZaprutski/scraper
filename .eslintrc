{
    "extends": "airbnb-base",
    "plugins": ["jest"],
    "env": {
        "jest/globals": true,
        "browser": true
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": "off",
        "max-len": [
            "error",
            120,
            {
                "ignoreStrings": true,
                "ignoreRegExpLiterals": true
            }
        ],
        "no-await-in-loop": "off"
    }
}
